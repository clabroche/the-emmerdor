{
  "name": "the-emmerdor",
  "version": "0.1.6",
  "description": "With this module, you have the possibility to configure the sounds played when a given user enters a discord channel.",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd server && npm i && cd ../front && npm i",
    "serve": "stack-monitor ./stack.js",
